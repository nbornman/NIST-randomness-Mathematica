            RowBox[{"j", "++"}], ";", " ", 
            RowBox[{"Continue", "[", "]"}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"words", "[", "tmp", "]"}], " ", "=", " ", "tmp"}], ";", 
         "\[IndentingNewLine]", " ", 
         RowBox[{"++", "wobs"}], ";", "\[IndentingNewLine]", 
         RowBox[{"i", " ", "+=", " ", 
          RowBox[{"j", "+", "1"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
