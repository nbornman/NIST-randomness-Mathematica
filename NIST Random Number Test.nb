      "\[IndentingNewLine]", 
      RowBox[{"wobs", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"i", " ", "+", "j"}], "  ", "\[LessEqual]", " ", "n"}], ",", 
        "\[IndentingNewLine]", 
