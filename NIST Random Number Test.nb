       RowBox[{"\[Mu]", " ", "=", " ", "69586.25"}], ",", " ", 
       RowBox[{"sigma", " ", "=", " ", "70.448718"}], ",", " ", "words", ",", 
       " ", "wordlen"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"s2", " ", "=", " ", 
