Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"BinExp", "[", 
    RowBox[{"num_", ",", "d_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"n", ",", "L"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"d", ">", "$MaxPrecision"}], ",", 
        RowBox[{"$MaxPrecision", "=", "d"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"n", "=", 
       RowBox[{"N", "[", 
        RowBox[{"num", ",", "d"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"L", "=", 
       RowBox[{"First", "[", 
        RowBox[{"RealDigits", "[", 
         RowBox[{"n", ",", "2"}], "]"}], "]"}]}]}]}], "]"}]}], ";"}]], "Input",\

 InitializationCell->True,
